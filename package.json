{
  "name": "opengrab",
  "version": "1.0.0",
  "description": "Favicon & Open Graph image API service. Fetch favicons, OG images, and metadata from any URL.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest --forceExit",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "favicon",
    "opengraph",
    "og-image",
    "api",
    "metadata"
  ],
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.4",
    "lru-cache": "^10.1.0",
    "sharp": "^0.33.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
